<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/kakao2"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="450dp"
    >

    <ImageView
        android:id="@+id/kakao_bg"
        android:layout_width="wrap_content"
        android:layout_height="450dp"
        android:background="@drawable/kakao28_bg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />


    <ImageView
        android:id="@+id/kakao41_m1"
        android:layout_width="160dp"
        android:layout_height="35dp"
        android:layout_marginTop="268dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/kakao41_m1"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/kakao41_m2"
        android:layout_width="160dp"
        android:layout_height="35dp"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="36dp"
        android:layout_marginTop="5dp"
        android:visibility="invisible"
        android:background="@drawable/kakao41_m2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kakao41_m1" />

    <ImageView
        android:id="@+id/kakao41_h1"
        android:layout_width="210dp"
        android:layout_height="70dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/kakao41_h1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:visibility="invisible"

        />


    <TextView
        android:id="@+id/kao_tv1"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginLeft="80dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/malgun"
        android:gravity="start"
        android:textAlignment="textStart"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/kakao41_h1"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/kakao41_h2"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_marginStart="76dp"
        android:layout_marginLeft="84dp"
        android:background="@drawable/kakao41_h2"
        android:layout_marginTop="5dp"
        android:visibility="invisible"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kakao41_h1" />

    <ImageView
        android:id="@+id/kakao41_h3"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_marginStart="76dp"
        android:layout_marginLeft="84dp"
        android:background="@drawable/kakao41_h3"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="5dp"
        android:visibility="invisible"

        app:layout_constraintTop_toBottomOf="@+id/kakao41_h2" />

    <ImageView
        android:id="@+id/kakao41_h4"
        android:layout_width="150dp"
        android:layout_height="65dp"
        android:layout_marginStart="76dp"
        android:layout_marginLeft="84dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/kakao41_h4"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kakao41_h3"/>


</androidx.constraintlayout.widget.ConstraintLayout>
