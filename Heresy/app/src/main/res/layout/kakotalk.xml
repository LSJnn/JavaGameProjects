<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/kakao1"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="360dp"
    >

    <ImageView
        android:id="@+id/kakao_bg"
        android:layout_width="wrap_content"
        android:layout_height="360dp"
        android:background="@drawable/kakao_bcg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/kakao_date1"
        android:layout_width="130dp"
        android:layout_height="22dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/kakao_date1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageView
        android:id="@+id/kakao_h1"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="56dp"
        android:layout_marginRight="36dp"
        android:background="@drawable/kakao_h_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:visibility="visible"
        />

    <TextView
        android:id="@+id/kao_tv1"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_marginStart="70dp"
        android:layout_marginLeft="70dp"
        android:fontFamily="@font/malgun"
        android:gravity="center"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/kakao_h1"
        app:layout_constraintEnd_toEndOf="@+id/kakao_h1"
        app:layout_constraintStart_toStartOf="@+id/kakao_h1" />

    <ImageView
        android:id="@+id/kakao_m1"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/kakao_m1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:visibility="invisible"

        />

    <ImageView
        android:id="@+id/kakao_m2"
        android:layout_width="90dp"
        android:layout_height="39dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:background="@drawable/kakao_m2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kakao_m1"
        android:visibility="invisible"
        />

    <ImageView
        android:id="@+id/kakao_h2"
        android:layout_width="240dp"
        android:layout_height="60dp"
        android:layout_marginTop="124dp"
        android:layout_marginRight="36dp"
        android:background="@drawable/kakao_h3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/kakao_h1"
        app:layout_constraintTop_toBottomOf="@+id/kakao_h1"
        android:visibility="invisible"
        />

    <ImageView
        android:id="@+id/kakao_h3"
        android:layout_width="185dp"
        android:layout_height="40dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/kakao_h0"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="@+id/kakao_h2"
        app:layout_constraintTop_toBottomOf="@+id/kakao_h2" />


</androidx.constraintlayout.widget.ConstraintLayout>
